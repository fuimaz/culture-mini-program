<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hk.culture.mini.program.mapping.NewsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hk.culture.mini.program.entity.News">
        <id column="TID" property="tid" />
        <result column="title" property="title" />
        <result column="content" property="content" />
        <result column="createTime" property="createTime" />
        <result column="brief" property="brief" />
        <result column="type" property="type" />
        <result column="backgroundImg" property="backgroundImg" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        TID, title, content, createTime, brief, type, backgroundImg
    </sql>

</mapper>
